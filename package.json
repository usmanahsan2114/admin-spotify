{
  "name": "shopify-admin",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo configuration for Shopify-like admin dashboard",
  "scripts": {
    "dev": "npm-run-all --parallel dev:backend dev:frontend dev:storefront",
    "dev:clean": "kill-port 5173 5174 3000 3001 || exit 0 && npm run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend run dev",
    "dev:storefront": "npm --prefix storefront run dev",
    "build": "npm --prefix frontend run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.5.0",
    "concurrently": "^9.1.0",
    "kill-port": "^3.0.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.22",
    "axios": "^1.13.2",
    "lucide-react": "^0.555.0",
    "postcss": "^8.5.6",
    "react-router-dom": "^6.23.1",
    "tailwindcss": "^4.1.17"
  }
}